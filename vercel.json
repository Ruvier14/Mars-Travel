{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "firstPage/**",
      "use": "@vercel/static"
    },
    {
      "src": "secPage/**",
      "use": "@vercel/static"
    },
    {
      "src": "thirPage/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/secPage/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/thirPage/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}