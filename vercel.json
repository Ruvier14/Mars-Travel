{
    "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "^/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "^/(.*).(js|css|json|png|jpg|jpeg|gif|svg|ico)$",
        "dest": "/$1.$2"
      },
      {
        "src": "^/(.*)",
        "dest": "/index.html"
      }
    ]
  }