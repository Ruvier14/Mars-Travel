{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/secPage/(.*)",
      "dest": "/secPage/$1"
    },
    {
      "src": "/thirPage/(.*)",
      "dest": "/thirPage/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}